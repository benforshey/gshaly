{
  "name": "gshaly",
  "version": "1.0.0",
  "description": "official website of the GS Haly Co",
  "main": "index.html",
  "dependencies": {
    "babel-polyfill": "6.26.0"
  },
  "devDependencies": {
    "autoprefixer": "7.2.6",
    "babel-core": "^6.26.3",
    "babel-loader": "7.1.2",
    "babel-minify-webpack-plugin": "0.3.0",
    "babel-preset-env": "^1.7.0",
    "node-sass": "^4.11.0",
    "onchange": "3.3.0",
    "parallelshell": "3.0.2",
    "postcss-cli": "5.0.0",
    "webpack": "3.11.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "style.autoprefixer": "postcss --use autoprefixer --replace ./static/dist/style/main.css",
    "style.sass": "node-sass --source-map true --output-style compressed ./static/src/style/main.scss ./static/dist/style/main.css",
    "style.build": "npm run style.sass && npm run style.autoprefixer",
    "style.watch": "onchange './static/src/style/**/*' -- npm run style.build",
    "script.build": "webpack",
    "build": "npm run style.build && npm run script.build && hugo",
    "watch": "npm run style.watch & npm run script.watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benforshey/gshaly.com.git"
  },
  "author": "",
  "license": "CC-BY-NC-ND-4.0",
  "bugs": {
    "url": "https://github.com/benforshey/gshaly.com/issues"
  },
  "homepage": "https://github.com/benforshey/gshaly.com#readme"
}
